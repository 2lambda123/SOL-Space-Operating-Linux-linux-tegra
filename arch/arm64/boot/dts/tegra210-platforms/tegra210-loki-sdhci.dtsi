/ {
	sdhci@700b0600 { /* SDMMC4 for EMMC on loki */
		tap-delay = <4>;
		trim-delay = <8>;
		mmc-ocr-mask = <0>;
		dqs-trim-delay = <17>;
		max-clk-limit = <200000000>;
		bus-width = <8>;
		id = <3>;
		built-in;
		compad-vref-3v3 = <0x7>;
		compad-vref-1v8 = <0x7>;
	};

	sdhci@700b0400 { /* SDMMC3 for 2nd Wifi on loki */
		tap-delay = <1>; /* SDMMC3 no stuff for Full size SD on foster */
		trim-delay = <3>;
		mmc-ocr-mask = <0>;
		max-clk-limit = <204000000>;
		ddr-clk-limit = <48000000>;
		bus-width = <4>;
		id = <2>;
		compad-vref-3v3 = <0x1>;
		compad-vref-1v8 = <0x2>;
		default-drv-type = <1>;
	};

	sdhci@700b0200 { /* SDMMC2 for Wifi on loki */
		tap-delay = <4>;
		trim-delay = <8>;
		mmc-ocr-mask = <0>;
		max-clk-limit = <204000000>;
		bus-width = <4>;
		id = <1>;
		compad-vref-3v3 = <0x7>;
		compad-vref-1v8 = <0x7>;
		default-drv-type = <1>;
	};

	sdhci@700b0000 { /* SDMMC1 for uSD card on loki */
		tap-delay = <2>;
		trim-delay = <2>;
		max-clk-limit = <204000000>;
		ddr-clk-limit = <48000000>;
		bus-width = <4>;
		id = <0>;
		mmc-ocr-mask = <3>;
		compad-vref-3v3 = <0x1>;
		compad-vref-1v8 = <0x2>;
		cd-gpios = <&gpio TEGRA_GPIO_PZ1 0>;
	};
};
