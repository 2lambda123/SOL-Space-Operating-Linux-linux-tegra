source drivers/devfreq/Kconfig

source arch/arm/mach-tegra/Kconfig

# FIXME: need revision
config ARCH_TEGRA_13x_SOC
        bool "Tegra 13x family SOC"
        depends on !ARCH_TEGRA_14x_SOC
        select ARCH_TEGRA_HAS_DUAL_CPU_CLUSTERS
        select CPU_V8
        select ARM_L1_CACHE_SHIFT_6
        select ARM_ARCH_TIMER
        select ARCH_TEGRA_HAS_SYMMETRIC_CPU_PWR_GATE
        select ARM_CPU_SUSPEND if PM
        select ARM_GIC
        select ARCH_REQUIRE_GPIOLIB
        select USB_ARCH_HAS_EHCI if USB_SUPPORT
        select USB_EHCI_TEGRA if USB_SUPPORT
        select USB_ULPI if USB_SUPPORT

# FIXME: need revision
config MACH_EXUMA
       bool "Exuma board"
       depends on ARCH_TEGRA_12x_SOC # FIXME: use 12x for now
       select TEGRA_FPGA_PLATFORM
       help
         Support for NVIDIA Exuma FPGA development platform
